{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "indent": ["error", 2],
        "no-underscore-dangle": "off"
    },
    "plugins": ["jest"]

}
